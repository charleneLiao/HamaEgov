<div class="list-multiple page-select <% if class_set %><%= class_set %><% end %>" data-index="<%= index %>" data-type="<%= $module %>" <% if func_set %> data-function="<%= func_set %>"<% end %> ><div class="inner">
	<div class="header"><div class="inner">
		<h4><span><a><% if header_text %><%= header_text %><% end %></a></span></h4>
	</div></div>
	<div class="content"><div class="inner">
		<ul data-index="1" data-child="2">
			<li class="first" data-index="1"><span><a href="#">最前頁</a></span></li>
			<li class="prev" data-index="2"><span><a href="#">上一頁</a></span></li>
		</ul>
		<ul data-index="2" data-child="2">
			<li class="next" data-index="1"><span><a href="#">下一頁</a></span></li>
			<li class="last" data-index="2"><span><a href="#">最後頁</a></span></li>
		</ul>
		<ul data-index="3" data-child="3">
			<li data-index="1">
				<span class="select">
					<select>
						<option selected>1</option>
					</select>
				</span>
			</li>
			<li data-index="2">
				<span class="number">
					<input type="number" min="1" max="999" value="20">
				</span>
			</li>
			<li data-index="3">
				<span class="count">275</span>
			</li>
		</ul>
	</div></div>
</div></div>