<!-- default 結束 -->

<div class="module-captions">//圖片================================================================</div>

<!-- block 圖片區塊列表 -->
	<%= render :partial => "/Erb/module/list-pic_block", :locals => { :index => 8, :class_set => nil, :func_set => "{'fadeSlider':{'auto':false,'delay':5000,'speed':300}}", :header_text => "輪播", :show_footer => true } %>
